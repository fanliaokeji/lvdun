            <left>0</left>
            <top>0</top>
            <width>350</width>
            <height>225</height>

            <topmost>0</topmost>
            <visible>0</visible>
            <enable>1</enable>
            <maxbox>0</maxbox>
            <minbox>1</minbox>
            <minwidth>50</minwidth>
            <minheight>50</minheight>
			<sysmenu>0</sysmenu>
			<appwindow>1</appwindow>
            <fps>30</fps>
			<blur>1</blur>
        </attr>
		 <eventlist>
            <event name="OnCreate" file="DownloadFileWnd.xml.lua" func="OnCreate"/>
            <event name="OnShowWindow" file="DownloadFileWnd.xml.lua" func="OnShowWindow"/>
        </eventlist>
