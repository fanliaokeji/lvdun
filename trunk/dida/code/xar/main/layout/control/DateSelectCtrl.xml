<xlue>
	<control class="DateSelectCtrl">
		<attr_def>
			
		</attr_def>
		<method_def>
			<GetYearText file="DateSelectCtrl.xml.lua" func="GetYearText"/>
			<SetYearText file="DateSelectCtrl.xml.lua" func="SetYearText"/>
			<GetMonthText file="DateSelectCtrl.xml.lua" func="GetMonthText"/>
			<SetMonthText file="DateSelectCtrl.xml.lua" func="SetMonthText"/>
		</method_def>
		<event_def>
			
		</event_def>
		<objtemplate>
			<children>
				<obj id="Combobox.Year" class="TipAddin.Button">
					<attr>
						<left>15</left>
						<top>0</top>
						<width>89</width>
						<height>20</height>
						<cursor>IDC_HAND</cursor>
						<NormalBkgID>DiDa.Combobox.Bkg.Long.Normal</NormalBkgID>
						<DownBkgID>DiDa.Combobox.Bkg.Long.Hover</DownBkgID>
						<HoverBkgID>DiDa.Combobox.Bkg.Long.Hover</HoverBkgID>
						<DisableBkgID>DiDa.Combobox.Bkg.Long.Normal</DisableBkgID>
						<TextColor>808080</TextColor>
						<TextFont>font.fzlt14</TextFont>
						<LeftTextPos>12</LeftTextPos>
						<TopTextPos>2</TopTextPos>
					</attr>
					<eventlist>
						<event name="OnLButtonUp" file="DateSelectCtrl.xml.lua" func="OnLButtonUpYearBox" />
						<event name="OnInitControl" file="DateSelectCtrl.xml.lua" func="OnInitYearBox" />
						<event name="OnMouseEnter" file="DateSelectCtrl.xml.lua" func="OnMouseEnterYearBox" />
						<event name="OnMouseLeave" file="DateSelectCtrl.xml.lua" func="OnMouseLeaveYearBox" />
					</eventlist>
				</obj>
				
				<obj id="Combobox.Month" class="TipAddin.Button">
					<attr>
						<left>15+89+5</left>
						<top>0</top>
						<width>62</width>
						<height>20</height>
						<cursor>IDC_HAND</cursor>
						<NormalBkgID>DiDa.Combobox.Bkg.Short.Normal</NormalBkgID>
						<DownBkgID>DiDa.Combobox.Bkg.Short.Hover</DownBkgID>
						<HoverBkgID>DiDa.Combobox.Bkg.Short.Hover</HoverBkgID>
						<DisableBkgID>DiDa.Combobox.Bkg.Short.Normal</DisableBkgID>
						<TextColor>808080</TextColor>
						<TextFont>font.fzlt14</TextFont>
						<LeftTextPos>10</LeftTextPos>
						<TopTextPos>2</TopTextPos>
					</attr>
					<eventlist>
						<event name="OnLButtonUp" file="DateSelectCtrl.xml.lua" func="OnLButtonUpMonthBox" />
						<event name="OnInitControl" file="DateSelectCtrl.xml.lua" func="OnInitMonthBox" />
					</eventlist>
				</obj>
				
				<obj id="Combobox.Festival" class="TipAddin.Button">
					<attr>
						<left>15+89+5+62+5</left>
						<top>0</top>
						<width>89</width>
						<height>20</height>
						<cursor>IDC_HAND</cursor>
						<NormalBkgID>DiDa.Combobox.Bkg.Long.Normal</NormalBkgID>
						<DownBkgID>DiDa.Combobox.Bkg.Long.Hover</DownBkgID>
						<HoverBkgID>DiDa.Combobox.Bkg.Long.Hover</HoverBkgID>
						<DisableBkgID>DiDa.Combobox.Bkg.Long.Normal</DisableBkgID>
						<TextColor>808080</TextColor>
						<TextFont>font.fzlt14</TextFont>
						<LeftTextPos>5</LeftTextPos>
						<TopTextPos>2</TopTextPos>
					</attr>
					<eventlist>
						<event name="OnLButtonUp" file="DateSelectCtrl.xml.lua" func="OnLButtonUpFestivalBox" />
						<event name="OnInitControl" file="DateSelectCtrl.xml.lua" func="OnInitFestivalBox" />
					</eventlist>
				</obj>
				
				<obj id="BackToToday.Text" class="TextObject">
					<attr>
						<left>father.width-60-20</left>
						<top>0</top>
						<width>60</width>
						<height>father.height</height>
						<cursor>IDC_HAND</cursor>
						<text>返回今天</text>
						<valign>center</valign>
						<halign>left</halign>
						<font>font.fzlt13</font>
						<textcolor>59ACFF</textcolor>
					</attr>
					<eventlist>
						<event name="OnLButtonUp" file="DateSelectCtrl.xml.lua" func="OnLClickToday" />
					</eventlist>
				</obj>
				
								
			</children>
			<eventlist>
				<event name="OnInitControl" file="DateSelectCtrl.xml.lua" func="OnInitRootCtrl" />
			</eventlist>
		</objtemplate>
	</control>
</xlue>